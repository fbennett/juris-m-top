                <else>
                  <text label-form="short" macro="std-locator-label"/>
                </else>
              </choose>
            </group>
          </if>
          <else-if match="any" type="bill book gazette graphic legislation legal_case motion_picture report song">
            <choose>
              <if match="none" type="bill gazette legislation">
                <group delimiter=":">
                  <number form="numeric" variable="volume"/>
                  <number variable="locator"/>
                </group>
              </if>
              <else>
                <text label-form="symbol" macro="std-locator-label"/>
              </else>
            </choose>
          </else-if>
          <else>
            <text label-form="short" macro="std-locator-non-page-label"/>
          </else>
        </choose>
      </if>
    </choose>
